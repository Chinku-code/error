.build:
  stage: build
  image:
    name: registry.dev.sbiepay.sbi:8443/ubi9/php-8.2:v1
    pull_policy: always

  script:
    - pwd
    - ls -la
    - |
        if [ -n "$VERSION" ]; then
          echo "version $VERSION"
        fi
    - php -v
    - ls -lrth

  artifacts:
    paths:
      - .
    when: on_success
    expire_in: 1 week
